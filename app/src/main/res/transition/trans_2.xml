<?xml version="1.0" encoding="utf-8"?>
<transitionSet
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:transitionOrdering="together">
    <changeBounds
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:duration="500"
        android:interpolator="@android:interpolator/linear">
    </changeBounds>
    <fade android:duration="250" android:fadingMode="fade_in" android:startDelay="1000" >
        <targets>
            <target android:targetId="@+id/textView4" />
            <target android:targetId="@+id/textView5" />
            <target android:targetId="@+id/textView6" />
            <target android:targetId="@+id/textView7" />
        </targets>
    </fade>
    <fade android:duration="1" android:fadingMode="fade_out" android:startDelay="1000" >
        <targets>
            <target android:targetId="@+id/textView4" />
            <target android:targetId="@+id/textView5" />
            <target android:targetId="@+id/textView6" />
            <target android:targetId="@+id/textView7" />
        </targets>
    </fade>
</transitionSet>
